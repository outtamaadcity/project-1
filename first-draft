secret_word = "giraffe"
guess = ""
guess_count = 0
lose = False
while lose == False:
    if guess != secret_word:
        guess = input("Enter guess: ")
        guess_count += 1
        print (guess_count)
        if guess_count >=3:
                lose == True
    if guess == secret_word: ("You win")
if lose == True:
    print ("You lose")
